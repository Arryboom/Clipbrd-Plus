<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:s="clr-namespace:System;assembly=mscorlib"
                    xmlns:local="clr-namespace:Clipbrd_Plus.Language">

    <!--功能菜单设置-->
    <s:String x:Key="SetWelcome">欢迎</s:String>
    <s:String x:Key="SetGeneralSetting">常规项设置</s:String>
    <s:String x:Key="SetClipSetting">剪切板设置</s:String>
    <s:String x:Key="SetHotKeySetting">快捷键设置</s:String>
    <s:String x:Key="SetPreviewSetting">预览设置</s:String>
    <s:String x:Key="SetScreenShotSetting">截屏设置</s:String>
    <s:String x:Key="SetAbout">关于</s:String>

    <!--常规设置-->
    <s:String x:Key="WelTitle">欢迎使用ClipBoard Plus</s:String>
    <s:String x:Key="GenStart">开机自启动</s:String>
    <s:String x:Key="GenPlayVoice">播放提示音</s:String>
    <s:String x:Key="GenBrowse">浏览</s:String>
    <s:String x:Key="GenListen">试听</s:String>
    <s:String x:Key="GenHistory">历史记录数量设置</s:String>
    <s:String x:Key="GenHistory2">(16-2048条)</s:String>
    <s:String x:Key="GenLanguage">选择您使用的语言</s:String>

    <!--剪切板设置-->
    <s:String x:Key="ClipText">记录剪切板文本历史</s:String>
    <s:String x:Key="ClipBitmap">记录剪切板图像历史</s:String>
    <s:String x:Key="ClipFile">记录剪切板文件历史</s:String>

    <!--快捷键设置-->
    <s:String x:Key="HotLastOne">切换到上一条记录</s:String>
    <s:String x:Key="HotLastOnePaste">切换到上一条记录并粘贴</s:String>
    <s:String x:Key="HotNextOne">切换到下一条记录</s:String>
    <s:String x:Key="HotNextOnePaste">切换到下一条记录并粘贴</s:String>
    <s:String x:Key="HotScreenShot">截取屏幕图像</s:String>
    <s:String x:Key="HotScreenShotSave">截取屏幕图像并保存</s:String>
    <s:String x:Key="HotPasteToWindow">贴图到窗口</s:String>
    <s:String x:Key="HotShowHide">显示/隐藏窗口上的贴图</s:String>
</ResourceDictionary>