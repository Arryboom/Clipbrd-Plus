<UserControl x:Class="Clipbrd_Plus.Hotkeysetting"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:local="clr-namespace:Clipbrd_Plus"
             mc:Ignorable="d" 
             d:DesignHeight="500" d:DesignWidth="800">
    <md:TransitioningContent x:Name="TransitioningContentMenu" OpeningEffect="{md:TransitionEffect SlideInFromLeft, Duration=0:0:0.3}" Grid.ColumnSpan="2" Grid.RowSpan="3">
        <Grid Background="White" >


            <Grid Margin="0, 0" Height="40" VerticalAlignment="Top">
                <StackPanel HorizontalAlignment="Left">
                    <TextBlock Text="快捷键设置" Margin="10" FontSize="20" Foreground="#FF555555"></TextBlock>
                </StackPanel>
            </Grid>

            <Grid Margin="0,40,0,0" >
                <Grid.RowDefinitions>
                    <RowDefinition Height="45"/>
                    <RowDefinition Height="45"/>
                    <RowDefinition Height="45"/>
                    <RowDefinition Height="45"/>
                    <RowDefinition Height="45"/>
                    <RowDefinition Height="45"/>
                    <RowDefinition Height="45"/>
                    <RowDefinition Height="45"/>
                    <RowDefinition Height="45"/>
                    <RowDefinition Height="45"/>
                    <RowDefinition Height="45"/>
                </Grid.RowDefinitions>

                <WrapPanel HorizontalAlignment="Left"  Grid.Row="0" VerticalAlignment="Center">
                    <md:PackIcon Kind="KeyboardArrowLeft" Margin="40,0,0,0" Height="24" Width="24" Foreground="#34b4c8" VerticalAlignment="Center"></md:PackIcon>
                    <TextBlock Text="切换到上一条记录" Margin="0,10" Width="200" FontSize="16" Foreground="#34b4c8" VerticalAlignment="Center"></TextBlock>
                    <TextBox Width="150" Height="32" Margin="10,0,10,0" BorderBrush="#34b4c8" Padding="0" BorderThickness="1" CaretBrush="DarkCyan" FontSize="16" VerticalAlignment="Center"></TextBox>
                    <Button Margin="0" Width="90" Height="32" Background="#34b4c8" BorderBrush="#34b4c8" >
                        <StackPanel Orientation="Horizontal" Width="60" Height="24" Margin="0" VerticalAlignment="Center">
                            <md:PackIcon Kind="Restart" Height="24"  Width="24"></md:PackIcon>
                            <TextBlock Text="重置" FontSize="16" Width="40" Margin="0,1"></TextBlock>
                        </StackPanel>
                    </Button>
                </WrapPanel>

                <WrapPanel HorizontalAlignment="Left"  Grid.Row="1" VerticalAlignment="Center">
                    <md:PackIcon Kind="Sticker" Margin="40,0,0,0" Height="24" Width="24" Foreground="#34b4c8" VerticalAlignment="Center"></md:PackIcon>
                    <TextBlock Text="切换到上一条记录并粘贴" Margin="0,10" Width="200" FontSize="16" Foreground="#34b4c8" VerticalAlignment="Center"></TextBlock>
                    <TextBox Width="150" Height="32" Margin="10,0,10,0" BorderBrush="#34b4c8" Padding="0" BorderThickness="1" CaretBrush="DarkCyan" FontSize="16" ></TextBox>
                    <Button Margin="0" Width="90" Height="32" Background="#34b4c8" BorderBrush="#34b4c8" >
                        <StackPanel Orientation="Horizontal" Width="60" Height="24" Margin="0" VerticalAlignment="Center">
                            <md:PackIcon Kind="Restart" Height="24"  Width="24"></md:PackIcon>
                            <TextBlock Text="重置" FontSize="16" Width="40" Margin="0,1"></TextBlock>
                        </StackPanel>
                    </Button>
                </WrapPanel>


                <WrapPanel HorizontalAlignment="Left"  Grid.Row="2" VerticalAlignment="Center">
                    <md:PackIcon Kind="KeyboardArrowRight" Margin="40,0,0,0" Height="24" Width="24" Foreground="#34b4c8" VerticalAlignment="Center"></md:PackIcon>
                    <TextBlock Text="切换到下一条记录" Margin="0,10" Width="200" FontSize="16" Foreground="#34b4c8" VerticalAlignment="Center"></TextBlock>
                    <TextBox Width="150" Height="32" Margin="10,0,10,0" BorderBrush="#34b4c8" Padding="0" BorderThickness="1" CaretBrush="DarkCyan" FontSize="16"></TextBox>
                    <Button Margin="0" Width="90" Height="32" Background="#34b4c8" BorderBrush="#34b4c8" >
                        <StackPanel Orientation="Horizontal" Width="60" Height="24" Margin="0" VerticalAlignment="Center">
                            <md:PackIcon Kind="Restart" Height="24"  Width="24"></md:PackIcon>
                            <TextBlock Text="重置" FontSize="16" Width="40" Margin="0,1"></TextBlock>
                        </StackPanel>
                    </Button>
                </WrapPanel>

                <WrapPanel HorizontalAlignment="Left"  Grid.Row="3" VerticalAlignment="Center">
                    <md:PackIcon Kind="Sticker" Margin="40,0,0,0" Height="24" Width="24" Foreground="#34b4c8" VerticalAlignment="Center"></md:PackIcon>
                    <TextBlock Text="切换到下一条记录并粘贴" Margin="0,10" Width="200" FontSize="16" Foreground="#34b4c8" VerticalAlignment="Center"></TextBlock>
                    <TextBox Width="150" Height="32" Margin="10,0,10,0" BorderBrush="#34b4c8" Padding="0" BorderThickness="1" CaretBrush="DarkCyan" FontSize="16"></TextBox>
                    <Button Margin="0" Width="90" Height="32" Background="#34b4c8" BorderBrush="#34b4c8" >
                        <StackPanel Orientation="Horizontal" Width="60" Height="24" Margin="0" VerticalAlignment="Center">
                            <md:PackIcon Kind="Restart" Height="24"  Width="24"></md:PackIcon>
                            <TextBlock Text="重置" FontSize="16" Width="40" Margin="0,1"></TextBlock>
                        </StackPanel>
                    </Button>
                </WrapPanel>

                <WrapPanel HorizontalAlignment="Left"  Grid.Row="4" VerticalAlignment="Center">
                    <md:PackIcon Kind="MonitorScreenshot" Margin="40,0,0,0" Height="24" Width="24" Foreground="#34b4c8" VerticalAlignment="Center"></md:PackIcon>
                    <TextBlock Text="截取屏幕图像" Margin="0,10" Width="200" FontSize="16" Foreground="#34b4c8" VerticalAlignment="Center"></TextBlock>
                    <TextBox Width="150" Height="32" Margin="10,0,10,0" BorderBrush="#34b4c8" Padding="0" BorderThickness="1" CaretBrush="DarkCyan" FontSize="16"></TextBox>
                    <Button Margin="0" Width="90" Height="32" Background="#34b4c8" BorderBrush="#34b4c8" >
                        <StackPanel Orientation="Horizontal" Width="60" Height="24" Margin="0" VerticalAlignment="Center">
                            <md:PackIcon Kind="Restart" Height="24"  Width="24"></md:PackIcon>
                            <TextBlock Text="重置" FontSize="16" Width="40" Margin="0,1"></TextBlock>
                        </StackPanel>
                    </Button>
                </WrapPanel>

                <WrapPanel HorizontalAlignment="Left"  Grid.Row="5" VerticalAlignment="Center">
                    <md:PackIcon Kind="ContentSave" Margin="40,0,0,0" Height="24" Width="24" Foreground="#34b4c8" VerticalAlignment="Center"></md:PackIcon>
                    <TextBlock Text="截取屏幕图像并保存" Margin="0,10" Width="200" FontSize="16" Foreground="#34b4c8" VerticalAlignment="Center"></TextBlock>
                    <TextBox Width="150" Height="32" Margin="10,0,10,0" BorderBrush="#34b4c8" Padding="0" BorderThickness="1" CaretBrush="DarkCyan" FontSize="16"></TextBox>
                    <Button Margin="0" Width="90" Height="32" Background="#34b4c8" BorderBrush="#34b4c8" >
                        <StackPanel Orientation="Horizontal" Width="60" Height="24" Margin="0" VerticalAlignment="Center">
                            <md:PackIcon Kind="Restart" Height="24"  Width="24"></md:PackIcon>
                            <TextBlock Text="重置" FontSize="16" Width="40" Margin="0,1"></TextBlock>
                        </StackPanel>
                    </Button>
                </WrapPanel>

                <WrapPanel HorizontalAlignment="Left"  Grid.Row="6" VerticalAlignment="Center">
                    <md:PackIcon Kind="PictureInPictureBottomRight" Margin="40,0,0,0" Height="24" Width="24" Foreground="#34b4c8" VerticalAlignment="Center"></md:PackIcon>
                    <TextBlock Text="贴图到窗口" Margin="0,10" FontSize="16" Width="200" Foreground="#34b4c8" VerticalAlignment="Center"></TextBlock>
                    <TextBox Width="150" Height="32" Margin="10,0,10,0" BorderBrush="#34b4c8" Padding="0" BorderThickness="1" CaretBrush="DarkCyan" FontSize="16"></TextBox>
                    <Button Margin="0" Width="90" Height="32" Background="#34b4c8" BorderBrush="#34b4c8" >
                        <StackPanel Orientation="Horizontal" Width="60" Height="24" Margin="0" VerticalAlignment="Center">
                            <md:PackIcon Kind="Restart" Height="24"  Width="24"></md:PackIcon>
                            <TextBlock Text="重置" FontSize="16" Width="40" Margin="0,1"></TextBlock>
                        </StackPanel>
                    </Button>
                </WrapPanel>

                <WrapPanel HorizontalAlignment="Left"  Grid.Row="7" VerticalAlignment="Center">
                    <md:PackIcon Kind="Eye" Margin="40,0,0,0" Height="24" Width="24" Foreground="#34b4c8" VerticalAlignment="Center"></md:PackIcon>
                    <TextBlock Text="显示/隐藏 窗口上的贴图" Margin="0,10" FontSize="16" Width="200" Foreground="#34b4c8" VerticalAlignment="Center"></TextBlock>
                    <TextBox Width="150" Height="32" Margin="10,0,10,0" BorderBrush="#34b4c8" Padding="0" BorderThickness="1" CaretBrush="DarkCyan" FontSize="16" ></TextBox>
                    <Button Margin="0" Width="90" Height="32" Background="#34b4c8" BorderBrush="#34b4c8" >
                        <StackPanel Orientation="Horizontal" Width="60" Height="24" Margin="0" VerticalAlignment="Center">
                            <md:PackIcon Kind="Restart" Height="24"  Width="24"></md:PackIcon>
                            <TextBlock Text="重置" FontSize="16" Width="40" Margin="0,1"></TextBlock>
                        </StackPanel>
                    </Button>
                </WrapPanel>
            </Grid>





        </Grid>
    </md:TransitioningContent>
</UserControl>
